<Application x:Class="Okuma.Scout.TestApp.net40.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Okuma.Scout.TestApp.net40"
             xmlns:helpers="clr-namespace:Okuma.Scout.TestApp.net40.Helpers"
             StartupUri="Views/MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Brushes.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <helpers:LicenseExipreDateConverter x:Key="ScoutLicenseExipreDateConverter" />
            <helpers:LicenseExpiresBoolConverter x:Key="FlexNetLicenseExpiresBoolConverter" />
            <helpers:HexToBinConverter x:Key="HexStringToBinaryStringConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
